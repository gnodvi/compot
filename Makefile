#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------

all:  
	(cd ext/r; make)
	(cd src/h; make)
	touch MAKE

test:  
	(cd ext/r; M)
	(cd src/h; M)

#--------------------------------------------

clean:  
	(cd ext/r; make clean)
	(cd src/h; make clean)
	(rm -f MAKE)

