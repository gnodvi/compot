#MAIN_SRC_DIR = .

include ./Make.mak


OTHERS_DIR = ../others/m
TESTER_DIR = ../tester/m

#-------------------------------------------------------------------------------


all:  OTHERS__  TESTER__ 

#-------------------------------------------------------------------------------

OTHERS__ :
	(cd $(OTHERS_DIR); make)

TESTER__ :
	(cd $(TESTER_DIR); make)

#-------------------------------------------------------------------------------

clean : 
	(cd $(OTHERS_DIR); make clean)
	(cd $(TESTER_DIR); make clean)

