
#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------

MAIN_SRC_DIR = .

include $(MAIN_SRC_DIR)/Make.mak


GAUL-C_DIR  = $(MAIN_SRC_DIR)/gaul-c
GAUL-L_DIR  = $(MAIN_SRC_DIR)/gaul-l

#-------------------------------------------------------------------------------


all:  GAUL-C__  GAUL-L__ 

#-------------------------------------------------------------------------------

GAUL-C__ :
	(cd $(GAUL-C_DIR); make)

GAUL-L__ :
	(cd $(GAUL-L_DIR); make)

#-------------------------------------------------------------------------------


clean : 
	(cd $(GAUL-C_DIR); make clean)
	(cd $(GAUL-L_DIR); make clean)

#-------------------------------------------------------------------------------
#-------------------------------------------------------------------------------
