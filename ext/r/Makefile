#MAIN_SRC_DIR = .

include ../../Make.mak


ASDF-L_DIR = ../asdf-l		
PLOTER_DIR = ../ploter		

#-------------------------------------------------------------------------------


all:  ASDF-L__ PLOTER__  

#-------------------------------------------------------------------------------

ASDF-L__ :
	(cd $(ASDF-L_DIR); make)

PLOTER__ :
	(cd $(PLOTER_DIR); make)

#-------------------------------------------------------------------------------

clean : 
	(cd $(ASDF-L_DIR); make clean)
	(cd $(PLOTER_DIR); make clean)

#-------------------------------------------------------------------------------
