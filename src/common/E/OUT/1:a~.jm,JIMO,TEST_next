balance = 0
name    = Bob Smith
balance = 88

class_method: methodname = see vars =  body = 
    set balance
  
class_method: methodname = deposit vars = amount body = 
    set balance [+ $balance $amount]
  
class_method: methodname = seename vars =  body = 
    set name
  
