
TEST_dict_class

c1 = ::G_DICT_0

dict_print ::G_DICT_0 = 
  classname       = C1
  baseclasses     = {}
  classvars       {
    ;#    = _это_перечисление_переменных
    name1 = C1-name
  }
  constructorbody = 
    # а это конструктор класса 
    puts "C1 constructor ......."
    puts "name = [GET $obj name1]"
  


c2 = ::G_DICT_1

dict_print ::G_DICT_1 = 
  classname       = C2
  baseclasses     = C1
  classvars       {
    name1 = C2-name
    name2 = C2-name
  }
  constructorbody {
    puts = args = $args
  }


-----------------------
C1 constructor .......
name = C1-name

dict_print ::G_DICT_2 = 
  name1     = C1-name
  classname = C1

-----------------------
