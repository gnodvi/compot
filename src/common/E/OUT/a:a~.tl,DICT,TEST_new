
ppp = a b c {d e  f g}
ppp = a b c {d e  f g}

------------------------------------------

ppp = a b c {d e  f g}
ppp = a b c {d e  f g}
ppp = a b c {d e  f g}

ppp(a)   = b
ppp(c)   = d e  f g
ppp(c d) = e
ppp(c f) = g

ppp(a)   = h
ppp(c)   = i

ppp = a h c i

------------------------------------------

ppp = a h c {j k l m}

ppp expand = a h c {j k l m}
ppp(c)   = j k l m
ppp(c l) = m


ppp expand = a h c {j k l NEW}
ppp(c)   = j k l NEW
ppp expand = a h c {j {o p} l NEW}

ppp(c j o) = NEWEST

ppp(c j) = o NEWEST

------------------------------------------

dict_expand = a h c {j {o NEWEST} l NEW}

------------------------------------------


dict_print ::G_DICT_3 = 
  a           = b
  c           {
    d = e
    f {
      g = h
    }
  }
  verylongkey = 3



dict_print ::G_DICT_3 = 
  a           = b
  c           {
    d = ::G_DICT_4
    f {
      g = h
    }
  }
  verylongkey = 3

#        d ..............................
#

dict_print ::G_DICT_4 = 
  AA = BB




