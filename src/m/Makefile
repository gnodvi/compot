#-------------------------------------------------------------------------------

#MAIN_SRC_DIR = .

include ./Make.mak


COMMON_DIR = ../common

TEST-S_DIR = ../others/m

#-------------------------------------------------------------------------------


all:  COMMON__  TEST-S__   

#-------------------------------------------------------------------------------

COMMON__ :
	(cd $(COMMON_DIR); make)

TEST-S__ :
	(cd $(TEST-S_DIR); make)

#-------------------------------------------------------------------------------

clean : 
	(cd $(COMMON_DIR); make clean)
	(cd $(TEST-S_DIR); make clean)

#-------------------------------------------------------------------------------
