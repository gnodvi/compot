# -*-   mode: makefile ; coding: koi8   -*- ------------------------------------
#
#-------------------------------------------------------------------------------

#MAIN_SRC_DIR = .

include ./Make.mak


COMMON_DIR = ../common

OTHERS_DIR = ../others/m

#-------------------------------------------------------------------------------


all:  COMMON__  OTHERS__   

#-------------------------------------------------------------------------------

COMMON__ :
	(cd $(COMMON_DIR); make)

OTHERS__ :
	(cd $(OTHERS_DIR); make)

#-------------------------------------------------------------------------------

clean : 
	(cd $(COMMON_DIR); make clean)
	(cd $(OTHERS_DIR); make clean)

#-------------------------------------------------------------------------------
