#-------------------------------------------------------------------------------
#  
#  
#-------------------------------------------------------------------------------



GCC   = LANG=C gcc -O2

#-------------------------------------------------------------------------------

PROGS = j_main  

all: $(PROGS) 


#-------------------------------------------------------------------------------

J_OBJS =  T/j-comm.o T/j-intr.o T/j-scan.o T/j-util.o 


T/j-comm.o: j-comm_.c j-comm_.h
	$(GCC) -o $@ -c $<


T/j-intr.o: j-intr_.c j-glob_.h
	$(GCC) -o $@ -c $<

T/j-scan.o: j-scan_.c j-glob_.h
	$(GCC) -o $@ -c $<
 
T/j-util.o: j-util_.c j-glob_.h
	$(GCC) -o $@ -c $<



T/j_main.o: j_main_.c j-comm_.h j-glob_.h 
	$(GCC) -o $@ -c $<


j_main:	T/j_main.o $(J_OBJS)
	gcc -o $@ $^  -lm 

#-------------------------------------------------------------------------------

clean : 
	rm -f T/*.o $(PROGS)

#-------------------------------------------------------------------------------
# 
#-------------------------------------------------------------------------------
